<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".home.HomeFragment" android:id="@+id/frameLayout5">
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <Button
                android:text="@string/note"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:minWidth="170dp"
                android:minHeight="60dp"
                android:id="@+id/note_button"
                app:layout_constraintTop_toBottomOf="@+id/guideline3"
                app:layout_constraintStart_toEndOf="@+id/guideline2" android:layout_marginStart="15dp"
                android:layout_marginBottom="20dp" app:layout_constraintBottom_toTopOf="@+id/guideline4"
                app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="30dp"/>
        <Button
                android:text="@string/new_pc_car"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:minWidth="170dp"
                android:minHeight="60dp"
                android:id="@+id/pcCar_button"
                android:layout_marginTop="1dp"
                app:layout_constraintTop_toBottomOf="@+id/guideline3"
                app:layout_constraintStart_toEndOf="@+id/guideline" android:layout_marginStart="15dp"
                app:layout_constraintEnd_toStartOf="@+id/guideline2" android:layout_marginEnd="15dp"
                android:layout_marginBottom="20dp" app:layout_constraintBottom_toTopOf="@+id/guideline4"/>
        <Button
                android:text="@string/parametri_vitali"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:minWidth="170dp"
                android:minHeight="60dp"
                android:id="@+id/param_vitali_button"
                android:layout_marginTop="2dp"
                app:layout_constraintTop_toBottomOf="@+id/guideline3" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="30dp"
                app:layout_constraintEnd_toStartOf="@+id/guideline"
                android:layout_marginEnd="15dp" android:layout_marginBottom="20dp"
                app:layout_constraintBottom_toTopOf="@+id/guideline4"/>
        <Button
                android:text="@string/storico"
                android:layout_width="0dp"
                android:minWidth="170dp"
                android:minHeight="60dp"
                android:id="@+id/storico_button"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="30dp" android:layout_marginTop="20dp"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toEndOf="@+id/guideline2"
                android:layout_marginStart="15dp" android:layout_height="0dp"
                app:layout_constraintBottom_toTopOf="@+id/guideline3"/>
        <Button
                android:text="@string/dett_missione"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:minWidth="170dp"
                android:minHeight="60dp"
                android:id="@+id/dett_missione_button"
                android:layout_marginBottom="1dp" app:layout_constraintBottom_toTopOf="@+id/guideline3"
                android:layout_marginTop="20dp" app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0" app:layout_constraintStart_toEndOf="@+id/guideline"
                android:layout_marginStart="15dp" app:layout_constraintEnd_toStartOf="@+id/guideline2"
                android:layout_marginEnd="15dp"/>
        <Button
                android:text="@string/stato_paziente"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:minWidth="170dp"
                android:minHeight="60dp"
                android:id="@+id/stato_paziente_button"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="30dp"
                android:layout_marginTop="20dp" app:layout_constraintTop_toTopOf="parent"
                android:layout_marginBottom="2dp" app:layout_constraintBottom_toTopOf="@+id/guideline3"
                android:layout_marginEnd="15dp" app:layout_constraintEnd_toStartOf="@+id/guideline"/>
        <com.google.android.material.tabs.TabLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                app:tabTextAppearance="@style/TabText"
                android:id="@+id/home_tabs"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.538"
                app:layout_constraintTop_toBottomOf="@+id/guideline4"
                app:layout_constraintBottom_toTopOf="@+id/guideline5">

            <com.google.android.material.tabs.TabItem
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/farmaci"/>

            <com.google.android.material.tabs.TabItem
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/manovre"/>

            <com.google.android.material.tabs.TabItem
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/trattamenti"/>
            <com.google.android.material.tabs.TabItem
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/complicanze_in_itinere"/>
        </com.google.android.material.tabs.TabLayout>
        <FrameLayout
                android:layout_height="0dp"
                android:id="@+id/tabFrame"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_width="match_parent" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/guideline5" android:layout_marginTop="5dp">

        </FrameLayout>
        <androidx.constraintlayout.widget.Guideline
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:layout_constraintGuide_percent="0.33"
                android:id="@+id/guideline"/>
        <androidx.constraintlayout.widget.Guideline
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:layout_constraintGuide_percent="0.66"
                android:id="@+id/guideline2"/>
        <androidx.constraintlayout.widget.Guideline
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:layout_constraintGuide_percent="0.12"
                android:id="@+id/guideline3"/>
        <androidx.constraintlayout.widget.Guideline
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:layout_constraintGuide_percent="0.24"
                android:id="@+id/guideline4"/>
        <androidx.constraintlayout.widget.Guideline
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:layout_constraintGuide_percent="0.34"
                android:id="@+id/guideline5"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>