<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   style="@style/BackgroundFragmentStyle"
                                                   tools:context=".dialog.PatientStatusDialogFragment"
                                                   android:id="@+id/frameLayout4">
    <TextView
            android:text="@string/stato_paziente"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/DialogTitleTextStyle"
            android:id="@+id/textView32"
            android:layout_marginTop="@dimen/verticalMargin"
            android:layout_marginBottom="@dimen/verticalMargin"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/patient_image_button"
            android:src="@drawable/ic_close_24dp"
            android:backgroundTint="@color/common_google_signin_btn_tint"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>
    <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/scrollView"
            android:layout_marginTop="45dp"
            app:layout_constraintTop_toBottomOf="@id/textView32"
            app:layout_constraintBottom_toBottomOf="parent">
        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/horizontalMargin"
                android:layout_marginEnd="@dimen/horizontalMargin">
            <Button
                    android:text="@string/penetrante"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/piercing_button"
                    style="@style/SelectableButtonStyle"
                    android:layout_marginTop="@dimen/verticalMargin"
                    android:layout_marginBottom="@dimen/verticalMargin"
                    app:layout_constraintTop_toTopOf="@+id/closed_button"
                    app:layout_constraintBottom_toBottomOf="@+id/closed_button"
                    app:layout_constraintStart_toEndOf="@+id/closed_button"
                    app:layout_constraintEnd_toEndOf="parent"/>
            <Button
                    android:text="@string/tetraparesi"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/tetraparesis_button"
                    style="@style/SelectableButtonStyle"
                    android:layout_marginTop="@dimen/verticalMargin"
                    android:layout_marginBottom="@dimen/verticalMargin"
                    app:layout_constraintTop_toBottomOf="@+id/sunken_button"
                    app:layout_constraintBottom_toTopOf="@id/constraintLayout11"
                    app:layout_constraintStart_toEndOf="@+id/paraparesi_button"
                    app:layout_constraintEnd_toStartOf="@+id/paresthesia_button"/>
            <Button
                    android:text="@string/paraparesi"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/paraparesi_button"
                    style="@style/SelectableButtonStyle"
                    android:layout_marginTop="@dimen/verticalMargin"
                    android:layout_marginBottom="@dimen/verticalMargin"
                    app:layout_constraintTop_toTopOf="@+id/tetraparesis_button"
                    app:layout_constraintBottom_toBottomOf="@+id/tetraparesis_button"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/tetraparesis_button"/>
            <androidx.constraintlayout.widget.Guideline
                    android:orientation="vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintGuide_percent="0.66"
                    android:id="@+id/guideline63"/>
            <Button
                    android:text="@string/parestesia"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/paresthesia_button"
                    style="@style/SelectableButtonStyle"
                    android:layout_marginTop="@dimen/verticalMargin"
                    android:layout_marginBottom="@dimen/verticalMargin"
                    app:layout_constraintTop_toTopOf="@+id/tetraparesis_button"
                    app:layout_constraintBottom_toBottomOf="@+id/tetraparesis_button"
                    app:layout_constraintStart_toEndOf="@id/tetraparesis_button"
                    app:layout_constraintEnd_toEndOf="parent"/>
            <Switch
                    android:text="@string/casco_cintura"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/helmet_belt_switch"
                    android:theme="@style/RadioStyle"
                    android:layout_marginTop="@dimen/verticalMargin"
                    android:layout_marginBottom="@dimen/verticalMargin"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/piercing_button"
                    app:layout_constraintBottom_toTopOf="@+id/external_hemorrage_switch"/>
            <Switch
                    android:text="@string/emorragia_esterna_in_atto"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/external_hemorrage_switch"
                    android:theme="@style/RadioStyle"
                    android:layout_marginTop="@dimen/verticalMargin"
                    android:layout_marginBottom="@dimen/verticalMargin"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/helmet_belt_switch"
                    app:layout_constraintBottom_toTopOf="@+id/airways_switch"/>
            <Switch
                    android:text="@string/vie_aeree_non_pervie"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/airways_switch"
                    android:theme="@style/RadioStyle"
                    android:layout_marginTop="@dimen/verticalMargin"
                    android:layout_marginBottom="@dimen/verticalMargin"
                    app:layout_constraintTop_toBottomOf="@+id/external_hemorrage_switch"
                    app:layout_constraintBottom_toTopOf="@+id/tachipnea_switch"/>
            <Switch
                    android:text="@string/tachipnea_dispnea"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/tachipnea_switch"
                    android:theme="@style/RadioStyle"
                    android:layout_marginTop="@dimen/verticalMargin"
                    android:layout_marginBottom="@dimen/verticalMargin"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/airways_switch"
                    app:layout_constraintBottom_toTopOf="@+id/volet_switch"/>
            <Switch
                    android:text="@string/volet_costale_deformit_torace"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/volet_switch"
                    android:theme="@style/RadioStyle"
                    android:layout_marginTop="@dimen/verticalMargin"
                    android:layout_marginBottom="@dimen/verticalMargin"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tachipnea_switch"
                    app:layout_constraintBottom_toTopOf="@+id/positive_button"/>
            <Switch
                    android:text="@string/bacino_instabile_deformato"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/unstable_pelvis_switch"
                    android:theme="@style/RadioStyle"
                    android:layout_marginTop="@dimen/verticalMargin"
                    android:layout_marginBottom="@dimen/verticalMargin"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/negative_button"
                    app:layout_constraintBottom_toTopOf="@+id/amputation_switch"/>
            <Switch
                    android:text="@string/amputazione_subamputazione"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/amputation_switch"
                    android:theme="@style/RadioStyle"
                    android:layout_marginTop="@dimen/verticalMargin"
                    android:layout_marginBottom="@dimen/verticalMargin"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/unstable_pelvis_switch"
                    app:layout_constraintBottom_toTopOf="@+id/otorragia_button"/>
            <Button
                    android:text="@string/otorragia"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/otorragia_button"
                    style="@style/SelectableButtonStyle"
                    android:layout_marginTop="@dimen/verticalMargin"
                    android:layout_marginBottom="@dimen/verticalMargin"
                    app:layout_constraintTop_toTopOf="@+id/sunken_button"
                    app:layout_constraintBottom_toBottomOf="@id/sunken_button"
                    app:layout_constraintStart_toEndOf="@+id/sunken_button"
                    app:layout_constraintEnd_toEndOf="parent"/>
            <Button
                    android:text="@string/chiuso"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/closed_button"
                    style="@style/SelectableButtonStyle"
                    android:layout_marginTop="@dimen/verticalMargin"
                    android:layout_marginBottom="@dimen/verticalMargin"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/guideline62"
                    app:layout_constraintEnd_toStartOf="@+id/piercing_button"/>
            <androidx.constraintlayout.widget.Guideline
                    android:orientation="vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintGuide_percent="0.33"
                    android:id="@+id/guideline62"/>
            <Button
                    android:text="@string/positivo"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/positive_button"
                    style="@style/SelectableButtonStyle"
                    android:layout_marginTop="@dimen/verticalMargin"
                    android:layout_marginBottom="@dimen/verticalMargin"
                    app:layout_constraintTop_toBottomOf="@+id/volet_switch"
                    app:layout_constraintBottom_toTopOf="@id/unstable_pelvis_switch"
                    app:layout_constraintStart_toEndOf="@+id/guideline62"
                    app:layout_constraintEnd_toStartOf="@+id/negative_button"/>
            <Button
                    android:text="@string/negativo"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/negative_button"
                    style="@style/SelectableButtonStyle"
                    android:layout_marginTop="@dimen/verticalMargin"
                    android:layout_marginBottom="@dimen/verticalMargin"
                    app:layout_constraintTop_toTopOf="@+id/positive_button"
                    app:layout_constraintBottom_toBottomOf="@+id/positive_button"
                    app:layout_constraintStart_toEndOf="@id/positive_button"
                    app:layout_constraintEnd_toEndOf="parent"/>
            <Button
                    android:text="@string/infossata"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/sunken_button"
                    style="@style/SelectableButtonStyle"
                    android:layout_marginTop="@dimen/verticalMargin"
                    android:layout_marginBottom="@dimen/verticalMargin"
                    app:layout_constraintTop_toBottomOf="@+id/amputation_switch"
                    app:layout_constraintBottom_toTopOf="@id/tetraparesis_button"
                    app:layout_constraintStart_toEndOf="@+id/guideline62"
                    app:layout_constraintEnd_toStartOf="@+id/otorragia_button"/>
            <TextView
                    android:text="@string/shock_index_o_shock_index_pedriatic_age_adjusted"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView30"
                    style="@style/BigTextStyle"
                    android:layout_marginTop="@dimen/verticalMargin"
                    android:layout_marginBottom="@dimen/verticalMargin"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/guideline62"
                    app:layout_constraintEnd_toStartOf="@+id/guideline63"/>
            <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/box"
                    android:id="@+id/constraintLayout11"
                    android:layout_marginTop="@dimen/verticalMargin"
                    android:layout_marginBottom="@dimen/verticalMargin"
                    android:paddingTop="@dimen/verticalMargin"
                    android:paddingBottom="@dimen/verticalMargin"
                    app:layout_constraintTop_toBottomOf="@+id/tetraparesis_button"
                    app:layout_constraintBottom_toTopOf="@id/textView30">

                <Button
                        android:text="@string/criterio_anatomico"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/anatomic_button"
                        style="@style/SelectableButtonStyle"
                        android:layout_marginTop="@dimen/verticalMargin"
                        android:layout_marginBottom="@dimen/verticalMargin"
                        app:layout_constraintWidth_percent="0.65"
                        app:layout_constraintTop_toBottomOf="@+id/physiologic_button"
                        app:layout_constraintBottom_toTopOf="@id/dynamic_button"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"/>
                <Button
                        android:text="@string/criterio_dinamico"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/dynamic_button"
                        style="@style/SelectableButtonStyle"
                        android:layout_marginTop="@dimen/verticalMargin"
                        android:layout_marginBottom="@dimen/verticalMargin"
                        app:layout_constraintWidth_percent="0.65"
                        app:layout_constraintTop_toBottomOf="@+id/anatomic_button"
                        app:layout_constraintBottom_toTopOf="@id/clinic_judgement_button"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"/>
                <Button
                        android:text="@string/giudizio_clinico"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/clinic_judgement_button"
                        style="@style/SelectableButtonStyle"
                        android:layout_marginTop="30dp"
                        android:layout_marginBottom="@dimen/verticalMargin"
                        app:layout_constraintWidth_percent="0.65"
                        app:layout_constraintTop_toBottomOf="@+id/dynamic_button"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"/>
                <Button
                        android:text="@string/criterio_fisiologico"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/physiologic_button"
                        style="@style/SelectableButtonStyle"
                        android:layout_marginTop="@dimen/verticalMargin"
                        android:layout_marginBottom="@dimen/verticalMargin"
                        app:layout_constraintWidth_percent="0.65"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toTopOf="@id/anatomic_button"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <TextView
                    android:text="0"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/shock_index_text"
                    style="@style/BigTextStyle"
                    android:layout_marginTop="@dimen/verticalMargin"
                    android:layout_marginBottom="@dimen/verticalMargin"
                    android:layout_marginStart="@dimen/horizontalMargin"
                    app:layout_constraintTop_toBottomOf="@+id/constraintLayout11"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/guideline63"/>
            <TextView
                    android:text="@string/ecofast"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView35"
                    style="@style/NormalTextStyle"
                    android:layout_marginTop="@dimen/verticalMargin"
                    android:layout_marginBottom="@dimen/verticalMargin"
                    app:layout_constraintTop_toTopOf="@+id/positive_button"
                    app:layout_constraintBottom_toBottomOf="@+id/positive_button"
                    app:layout_constraintStart_toStartOf="parent"/>
            <TextView
                    android:text="@string/frattura_cranica"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView36"
                    style="@style/NormalTextStyle"
                    android:layout_marginTop="@dimen/verticalMargin"
                    android:layout_marginBottom="@dimen/verticalMargin"
                    app:layout_constraintTop_toTopOf="@+id/sunken_button"
                    app:layout_constraintBottom_toBottomOf="@id/sunken_button"
                    app:layout_constraintStart_toStartOf="parent"/>
            <TextView
                    android:text="@string/tipo_trauma"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView37"
                    style="@style/NormalTextStyle"
                    android:layout_marginTop="8dp"
                    android:layout_marginBottom="@dimen/verticalMargin"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toTopOf="@+id/helmet_belt_switch"
                    app:layout_constraintStart_toStartOf="parent"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>