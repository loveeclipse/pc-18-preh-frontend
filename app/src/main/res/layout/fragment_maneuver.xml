<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".home.ManeuverFragment"
                                                   android:id="@+id/frameLayout">
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="546dp"
            android:layout_height="189dp" android:layout_marginTop="50dp"
            android:background="@drawable/box"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout2" android:id="@+id/constraintLayout3"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="10dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="10dp"
            app:layout_constraintHorizontal_bias="0.485">
        <EditText
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:ems="10"
                android:id="@+id/editText7" android:layout_marginTop="1dp"
                app:layout_constraintTop_toBottomOf="@+id/editText5"
                app:layout_constraintEnd_toStartOf="@+id/textView21" android:layout_marginEnd="21dp"/>
        <TextView
                android:text="Frequenza di cattura"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView18"
                app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="31dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="29dp"/>
        <TextView
                android:text="Amperaggio"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView19" android:layout_marginTop="34dp"
                app:layout_constraintTop_toBottomOf="@+id/textView18" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="82dp"/>
        <TextView
                android:text="bpm"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView20"
                app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="30dp"
                app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="59dp"/>
        <TextView
                android:text="mA"
                android:layout_width="wrap_content"
                android:layout_height="16dp"
                android:id="@+id/textView21" android:layout_marginEnd="66dp" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginTop="36dp" app:layout_constraintTop_toBottomOf="@+id/textView20"
        />
        <Button
                android:text="CONFERMA PACING TRANSCUTANEO"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/button2"
                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="17dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="135dp"
                app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="134dp"/>
        <EditText
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:ems="10"
                android:id="@+id/editText5"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="15dp" app:layout_constraintEnd_toStartOf="@+id/textView20"
                android:layout_marginEnd="20dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="581dp"
            android:layout_height="301dp" android:layout_marginTop="60dp"
            app:layout_constraintTop_toTopOf="parent" android:id="@+id/constraintLayout2"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="10dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="10dp">
        <Switch
                android:text="@string/sonda_vescicale"
                android:layout_width="500dp"
                android:layout_height="30dp"
                android:textSize="17dp"
                android:id="@+id/vescicale_switch" android:layout_marginTop="25dp"
                app:layout_constraintTop_toBottomOf="@+id/gastrica_switch" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="40dp" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="40dp"/>
        <Switch
                android:text="@string/sonda_gastrica"
                android:layout_width="500dp"
                android:layout_height="30dp"
                android:textSize="17dp"
                android:id="@+id/gastrica_switch" android:layout_marginTop="25dp"
                app:layout_constraintTop_toBottomOf="@+id/ces_switch" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="40dp" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="40dp"/>
        <Switch
                android:text="@string/cardioversione_elettrica_sincronizzata"
                android:layout_width="500dp"
                android:layout_height="30dp"
                android:textSize="17dp"
                android:id="@+id/ces_switch" android:layout_marginTop="25dp"
                app:layout_constraintTop_toBottomOf="@+id/imm_switch" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="40dp" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="40dp"/>
        <Switch
                android:text="@string/immobilizzazione"
                android:layout_width="500dp"
                android:layout_height="30dp"
                android:textSize="17dp"
                android:id="@+id/imm_switch" android:layout_marginTop="25dp"
                app:layout_constraintTop_toBottomOf="@+id/cervicale_switch" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="40dp" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="40dp"/>
        <Switch
                android:text="@string/collare_cervicale"
                android:layout_width="500dp"
                android:layout_height="30dp"
                android:textSize="17dp"
                android:id="@+id/cervicale_switch" app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="10dp" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="40dp" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="40dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <TextView
            android:text="@string/pacing_transcutaneo"
            android:layout_width="279dp"
            android:layout_height="27dp"
            android:textSize="20dp"
            android:textAllCaps="true"
            android:id="@+id/textView6"
            app:layout_constraintBottom_toTopOf="@+id/constraintLayout3" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp" android:layout_marginTop="53dp"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout2" app:layout_constraintVertical_bias="1.0"/>
</androidx.constraintlayout.widget.ConstraintLayout>