<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".home.DrugsFragment"
                                                   android:id="@+id/constraintLayout">
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_marginTop="80dp"
            android:background="@drawable/box"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout2" android:id="@+id/constraintLayout3"
            android:layout_height="202dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp">
        <Button
                android:text="@string/succinilcolina"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:id="@+id/succi_button"
                android:padding="16dp"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="20dp"
                android:backgroundTint="@android:color/holo_orange_light"
                android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
                android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/guideline14"
                android:layout_marginEnd="10dp" app:layout_constraintEnd_toStartOf="@+id/guideline25"/>
        <Button
                android:text="@string/midazolam"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:id="@+id/midazolam_button"
                android:padding="16dp"
                android:textSize="18sp"
                android:backgroundTint="@android:color/holo_orange_light" android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toTopOf="@+id/guideline14" android:layout_marginEnd="10dp"
                app:layout_constraintEnd_toStartOf="@+id/guideline26" android:layout_marginStart="10dp"
                app:layout_constraintStart_toStartOf="@+id/guideline25"/>
        <Button
                android:text="@string/fentanil"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:id="@+id/fentanil_button"
                android:padding="16dp"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="20dp"
                android:backgroundTint="@android:color/holo_orange_light" android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toTopOf="@+id/guideline14" android:layout_marginStart="10dp"
                app:layout_constraintStart_toStartOf="@+id/guideline26"/>
        <Button
                android:text="@string/ketamina"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:id="@+id/ketamina_button"
                android:padding="16dp"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="20dp"
                android:backgroundTint="@android:color/holo_green_dark"
                android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toBottomOf="@+id/guideline14"
                android:layout_marginTop="8dp" android:layout_marginEnd="10dp"
                app:layout_constraintEnd_toStartOf="@+id/guideline25"/>
        <Button
                android:text="@string/curaro"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:id="@+id/curaro_button"
                android:padding="16dp"
                android:textSize="18sp"
                android:backgroundTint="@android:color/holo_green_dark" android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="@+id/guideline14" android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="10dp"
                app:layout_constraintEnd_toStartOf="@+id/guideline26" android:layout_marginStart="10dp"
                app:layout_constraintStart_toStartOf="@+id/guideline25"/>
        <Button
                android:text="@string/acido_tranexamico"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:id="@+id/acido_button"
                android:padding="16dp"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="20dp"
                android:backgroundTint="@android:color/holo_green_dark"
                app:layout_constraintTop_toTopOf="@+id/guideline14" android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
                android:layout_marginStart="10dp" app:layout_constraintStart_toStartOf="@+id/guideline26"/>
        <androidx.constraintlayout.widget.Guideline
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:layout_constraintGuide_percent="0.5"
                android:id="@+id/guideline14"/>
        <androidx.constraintlayout.widget.Guideline
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/guideline25" app:layout_constraintGuide_percent="0.33"/>
        <androidx.constraintlayout.widget.Guideline
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:layout_constraintGuide_percent="0.65"
                android:id="@+id/guideline26"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="202dp"
            android:background="@drawable/box"
            app:layout_constraintTop_toTopOf="parent" android:id="@+id/constraintLayout2"
            android:layout_marginTop="40dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp">
        <Button
                android:text="@string/fibrinogeno"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:id="@+id/fibrinogeno_button"
                android:padding="16dp"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="20dp"
                android:backgroundTint="@android:color/holo_red_light"
                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="@+id/guideline24" android:layout_marginBottom="8dp"
                android:layout_marginStart="10dp" app:layout_constraintStart_toStartOf="@+id/guideline31"/>
        <Button
                android:text="@string/emazie_concentrate"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:id="@+id/emazie_button"
                android:padding="16dp"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="20dp"
                android:backgroundTint="@android:color/holo_red_light"
                app:layout_constraintTop_toTopOf="@+id/guideline24" android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
                android:layout_marginEnd="10dp" app:layout_constraintEnd_toStartOf="@+id/guideline31"/>
        <Button
                android:text="@string/mannitolo"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:id="@+id/mannitolo_button"
                android:padding="16dp"
                android:textSize="18sp"
                android:backgroundTint="@color/colorPrimary" android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toTopOf="@+id/guideline24" android:layout_marginEnd="10dp"
                app:layout_constraintEnd_toStartOf="@+id/guideline34" android:layout_marginStart="10dp"
                app:layout_constraintStart_toStartOf="@+id/guideline33"/>
        <Button
                android:text="@string/soluzione_ipertonica"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:id="@+id/ipertonica_button"
                android:padding="16dp"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="20dp"
                android:backgroundTint="@color/colorPrimary" android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toTopOf="@+id/guideline24" android:layout_marginStart="10dp"
                app:layout_constraintStart_toStartOf="@+id/guideline34"/>
        <Button
                android:text="@string/cristalloidi"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:id="@+id/cristalloidi_button"
                android:padding="16dp"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="20dp"
                android:backgroundTint="@color/colorPrimary" android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toTopOf="@+id/guideline24" android:layout_marginEnd="10dp"
                app:layout_constraintEnd_toStartOf="@+id/guideline33"/>
        <androidx.constraintlayout.widget.Guideline
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:layout_constraintGuide_percent="0.50"
                android:id="@+id/guideline24"/>
        <androidx.constraintlayout.widget.Guideline
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:layout_constraintGuide_percent="0.5"
                android:id="@+id/guideline31"/>
        <androidx.constraintlayout.widget.Guideline
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:layout_constraintGuide_percent="0.33"
                android:id="@+id/guideline33"/>
        <androidx.constraintlayout.widget.Guideline
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:layout_constraintGuide_percent="0.65"
                android:id="@+id/guideline34"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <TextView
            android:text="@string/infusioni"
            android:layout_width="99dp"
            android:layout_height="28dp"
            android:textSize="20dp"
            android:textAllCaps="true"
            android:id="@+id/textView5"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="15dp"
            app:layout_constraintBottom_toTopOf="@+id/constraintLayout2"
            android:layout_marginStart="10dp" app:layout_constraintStart_toStartOf="parent"/>
    <TextView
            android:text="@string/generici"
            android:layout_width="125dp"
            android:layout_height="27dp"
            android:textSize="20dp"
            android:textAllCaps="true"
            android:id="@+id/textView6"
            app:layout_constraintBottom_toTopOf="@+id/constraintLayout3"
            android:layout_marginTop="53dp"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout2"
            android:layout_marginStart="10dp" app:layout_constraintStart_toStartOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>