<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   style="@style/BackgroundFragmentStyle"
                                                   tools:context=".home.AnagraficFragment"
                                                   android:id="@+id/frameLayout6">
    <TextView
            android:text="@string/nome"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView33"
            style="@style/NormalTextStyle"
            android:layout_marginTop="80dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="45dp"/>
    <TextView
            android:text="@string/cognome"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView34"
            style="@style/NormalTextStyle"
            android:layout_marginTop="70dp"
            app:layout_constraintTop_toBottomOf="@+id/textView33"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="45dp"/>
    <TextView
            android:text="@string/residenza"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView38"
            style="@style/NormalTextStyle"
            android:layout_marginTop="70dp"
            app:layout_constraintTop_toBottomOf="@+id/textView34"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="45dp"/>
    <TextView
            android:text="@string/luogo_di_nascita"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView39"
            style="@style/NormalTextStyle"
            android:layout_marginTop="70dp"
            app:layout_constraintTop_toBottomOf="@+id/textView38"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="45dp"/>
    <TextView
            android:text="@string/data_di_nascita"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView40"
            style="@style/NormalTextStyle"
            android:layout_marginTop="70dp"
            app:layout_constraintTop_toBottomOf="@+id/textView39"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="45dp"/>
    <TextView
            android:text="@string/sesso"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView41"
            style="@style/TreatmentTextStyle"
            android:layout_marginTop="80dp"
            app:layout_constraintTop_toBottomOf="@+id/textView40"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="45dp"/>
    <TextView
            android:text="@string/anamnesi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView42"
            style="@style/BigTextStyle"
            android:layout_marginTop="80dp"
            app:layout_constraintTop_toBottomOf="@+id/textView41"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="90dp"/>
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/box"
            android:id="@+id/constraintLayout5"
            android:layout_marginBottom="60dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView42"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="80dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="80dp">

        <Switch
                android:text="@string/anticoagulanti"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/switch3"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="90dp"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="30dp"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="30dp"
                android:layout_marginBottom="30dp"
                app:layout_constraintBottom_toTopOf="@+id/switch4"/>
        <Switch
                android:text="@string/antipiastrine"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/switch4"
                android:layout_marginTop="30dp"
                app:layout_constraintTop_toBottomOf="@+id/switch3"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="30dp"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="90dp"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="30dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText"
            android:layout_marginTop="65dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginStart="250dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginEnd="40dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText2"
            android:layout_marginTop="50dp"
            app:layout_constraintTop_toBottomOf="@+id/editText"
            android:layout_marginStart="250dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginEnd="40dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText4"
            android:layout_marginTop="50dp"
            app:layout_constraintTop_toBottomOf="@+id/editText2"
            android:layout_marginStart="250dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginEnd="40dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText6"
            android:layout_marginTop="50dp"
            app:layout_constraintTop_toBottomOf="@+id/editText4"
            android:layout_marginStart="250dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginEnd="40dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="date"
            android:ems="10"
            android:id="@+id/editText8"
            app:layout_constraintTop_toBottomOf="@+id/editText6"
            android:layout_marginTop="50dp"
            android:layout_marginStart="250dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginEnd="40dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <RadioGroup
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="70dp"
            app:layout_constraintTop_toBottomOf="@+id/editText8"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="40dp"
            app:layout_constraintStart_toEndOf="@+id/textView41"
            android:layout_marginStart="40dp">
        <RadioButton
                android:text="M"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/radioButton2"
                android:layout_weight="1"/>
        <RadioButton
                android:text="F"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/radioButton"
                android:layout_weight="1"/>
    </RadioGroup>
</androidx.constraintlayout.widget.ConstraintLayout>